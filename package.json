    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "csv-parse": "^4.16.3",
    "date-fns": "^4.1.0",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "express-fileupload": "^1.2.1",
