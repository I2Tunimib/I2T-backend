  "private": true,
  "type": "module",
  "scripts": {
    "start-dev": "node node_modules/cross-env/src/bin/cross-env.js ENV=DEV npx nodemon --watch src --ignore tmp/ --experimental-specifier-resolution=node src/index.js",
